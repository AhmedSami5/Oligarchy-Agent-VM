{
  "dashboard": {
    "title": "DeMoD Agent System Overview",
    "tags": ["demod", "agent-system", "overview"],
    "timezone": "browser",
    "panels": [
      {
        "title": "System Uptime",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"demod-agent-system\"}",
            "legendFormat": "{{ instance }}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          },
          "overrides": []
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        },
        "id": "system-uptime",
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": false,
            "excludeUndefined": true
          },
          "textMode": "auto"
        },
        "pluginVersion": "10.2.0",
        "targets": [
          {
            "expr": "up{job=\"demod-agent-system\"}",
            "instant": true,
            "refId": "A"
          }
        ],
        "type": "stat"
      },
      {
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{ method }} {{ instance }}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 9,
          "x": 6,
          "y": 0
        },
        "id": "request-rate",
        "yAxes": [
          {
            "label": "Requests/sec"
          }
        ]
      },
      {
        "title": "Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "99th percentile"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 9,
          "x": 15,
          "y": 0
        },
        "id": "response-time",
        "yAxes": [
          {
            "label": "Seconds"
          }
        ]
      },
      {
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\"}[5m])",
            "legendFormat": "5xx errors"
          },
          {
            "expr": "rate(http_requests_total{status=~\"4..\"}[5m])",
            "legendFormat": "4xx errors"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "id": "error-rate",
        "yAxes": [
          {
            "label": "Errors/sec"
          }
        ]
      },
      {
        "title": "Active Agents",
        "type": "stat",
        "targets": [
          {
            "expr": "agent_active_count",
            "legendFormat": "Active Agents"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 8
        },
        "id": "active-agents",
        "type": "stat"
      },
      {
        "title": "Agent Task Queue",
        "type": "graph",
        "targets": [
          {
            "expr": "agent_task_queue_size",
            "legendFormat": "Queue Size"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 8
        },
        "id": "task-queue",
        "yAxes": [
          {
            "label": "Tasks"
          }
        ]
      },
      {
        "title": "Database Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "pg_stat_database_numbackends",
            "legendFormat": "Active Connections"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 16
        },
        "id": "db-connections",
        "yAxes": [
          {
            "label": "Connections"
          }
        ]
      },
      {
        "title": "CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "100 - (avg by(instance) (rate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "legendFormat": "{{ instance }}"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 16
        },
        "id": "cpu-usage",
        "yAxes": [
          {
            "label": "Percent",
            "max": 100
          }
        ]
      },
      {
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
            "legendFormat": "{{ instance }}"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 16
        },
        "id": "memory-usage",
        "yAxes": [
          {
            "label": "Percent",
            "max": 100
          }
        ]
      },
      {
        "title": "Agent Creation Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(agent_creation_success_total[5m])",
            "legendFormat": "Creation Success"
          },
          {
            "expr": "rate(agent_creation_failed_total[5m])",
            "legendFormat": "Creation Failures"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "id": "agent-creation",
        "yAxes": [
          {
            "label": "Operations/sec"
          }
        ]
      }
    ],
    "schemaVersion": 36,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "timezone": "",
    "title": "DeMoD Agent System - Production Monitoring"
  }
}